{
  "name": "TheResistance",
  "version": "0.1.0",
  "private": true,
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.34",
    "@babel/core": "^7.0.0-beta.34",
    "@babel/preset-env": "^7.0.0-beta.34",
    "babel-plugin-root-import": "^5.1.0",
    "babel-preset-react-native-stage-0": "^1.0.1",
    "fs-extra": "^4.0.2",
    "jest-expo": "^21.0.2",
    "prettier": "1.9.1",
    "react-test-renderer": "16.0.0-beta.5",
    "replace-in-file": "^3.0.0",
    "rimraf": "^2.6.2"
  },
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "rename": "node ./bin/rename.js",
    "start": "react-native start",
    "test": "node node_modules/jest/bin/jest.js --watch",
    "pretty": "yarn run prettier -- --write 'src/**/*.js'",
    "login": "firebase login",
    "preserve": "yarn package:functions",
    "serve": "firebase serve --only functions",
    "deploy:functions": "yarn run format:functions; yarn run package:functions; firebase deploy",
    "format:functions": "yarn run prettier --write ./firebaseFunctions/*.js",
    "prepackage:functions": "rimraf functions",
    "package:functions": "babel firebaseFunctions --out-dir functions --copy-files --ignore firebaseFunctions/node_modules",
    "postpackage:functions": "cd functions && yarn"
  },
  "jest": {
    "preset": "jest-expo"
  },
  "dependencies": {
    "babel-preset-env": "^1.6.1",
    "fbjs": "^0.8.16",
    "lodash": "^4.17.4",
    "obj-to-query-string": "^1.1.1",
    "prop-types": "^15.6.0",
    "react": "^16.0.0",
    "react-native": "^0.50.3",
    "react-native-firebase": "^3.1.1",
    "react-native-router-flux": "^4.0.0-beta.24"
  }
}
