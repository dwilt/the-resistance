{
  "name": "TheResistance",
  "version": "0.1.0",
  "private": true,
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.34",
    "@babel/core": "^7.0.0-beta.34",
    "@babel/preset-env": "^7.0.0-beta.34",
    "babel-eslint": "^8.0.3",
    "babel-preset-react-native-stage-0": "^1.0.1",
    "eslint": "^4.13.1",
    "eslint-plugin-react": "^7.5.1",
    "eslint-plugin-react-native": "^3.2.0",
    "fs-extra": "^4.0.2",
    "jest-expo": "^21.0.2",
    "prettier": "1.9.1",
    "react-test-renderer": "16.0.0-beta.5",
    "replace-in-file": "^3.0.0",
    "rimraf": "^2.6.2"
  },
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "build:ios": "react-native bundle --entry-file='index.js' --bundle-output='./ios/TheResistance/main.jsbundle' --dev=false --platform='ios' --assets-dest='./ios'",
    "rename": "node ./bin/rename.js",
    "start": "react-native start",
    "tidy": "yarn run pretty; yarn run lint",
    "lint": "./node_modules/.bin/eslint src -c .eslintrc --fix",
    "test": "node node_modules/jest/bin/jest.js --watch",
    "pretty": "yarn run prettier -- --write 'src/**/*.js' 'firebaseFunctions/*.js'",
    "login": "firebase login",
    "preserve": "yarn package:functions",
    "serve": "firebase serve --only functions",
    "deploy:functions": "yarn run package:functions; firebase deploy",
    "format:functions": "yarn run prettier --write ./firebaseFunctions/*.js",
    "prepackage:functions": "rimraf functions",
    "package:functions": "babel firebaseFunctions --out-dir functions --copy-files --ignore firebaseFunctions/node_modules",
    "postpackage:functions": "cd functions && yarn"
  },
  "jest": {
    "preset": "jest-expo"
  },
  "dependencies": {
    "babel-preset-env": "^1.6.1",
    "fbjs": "^0.8.16",
    "is-object": "^1.0.1",
    "json-style-converter": "^1.0.3",
    "lodash": "^4.17.4",
    "obj-to-query-string": "^1.1.1",
    "prop-types": "^15.6.0",
    "react": "^16.0.0",
    "react-native": "^0.50.3",
    "react-native-firebase": "^3.1.1",
    "react-native-router-flux": "^4.0.0-beta.24",
    "react-native-splash-screen": "^3.0.6",
    "react-native-vector-icons": "^4.4.3",
    "react-redux": "^5.0.6",
    "redux": "^3.7.2",
    "redux-saga": "^0.16.0",
    "reselect": "^3.0.1"
  },
  "rnpm": {
    "assets": [
      "src/assets/fonts"
    ]
  }
}
